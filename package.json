  },
  "devDependencies": {
    "all-contributors-cli": "^5.0.0",
    "eslint": "^4.19.1",
    "eslint-config-precure": "^2.7.0",
    "flow-bin": "^0.74.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5"
